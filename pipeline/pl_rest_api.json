{
	"name": "pl_rest_api",
	"properties": {
		"activities": [
			{
				"name": "act_Copydata",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['season']"
								},
								"sink": {
									"path": "$['MRData']['SeasonTable']['Seasons'][0]['season']"
								}
							},
							{
								"source": {
									"path": "['url']"
								},
								"sink": {
									"path": "$['MRData']['SeasonTable']['Seasons'][0]['url']"
								}
							},
							{
								"source": {
									"path": "$['MRData']['xmlns']"
								},
								"sink": {
									"path": "$['MRData']['xmlns']"
								}
							},
							{
								"source": {
									"path": "$['MRData']['series']"
								},
								"sink": {
									"path": "$['MRData']['series']"
								}
							},
							{
								"source": {
									"path": "$['MRData']['url']"
								},
								"sink": {
									"path": "$['MRData']['url']"
								}
							},
							{
								"source": {
									"path": "$['MRData']['limit']"
								},
								"sink": {
									"path": "$['MRData']['limit']"
								}
							},
							{
								"source": {
									"path": "$['MRData']['offset']"
								},
								"sink": {
									"path": "$['MRData']['offset']"
								}
							},
							{
								"source": {
									"path": "$['MRData']['total']"
								},
								"sink": {
									"path": "$['MRData']['total']"
								}
							}
						],
						"collectionReference": "$['MRData']['SeasonTable']['Seasons']"
					}
				},
				"inputs": [
					{
						"referenceName": "ds_RestResource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}